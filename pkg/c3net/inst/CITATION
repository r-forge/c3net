citHeader("To cite c3net in publications use:")

citEntry(entry = "Article",
  title        = "Inferring the conservative causal core of gene regulatory networks",
  author       = "Gokmen Altay and Frank emmert-Streib",
  journal      = "BMC Systems Biology",
  year         = "2010",
  volume       = "4",
  url 		   = "http://www.biomedcentral.com/1752-0509/4/132",
  textVersion  =
  paste("Gokmen Altay, Frank emmert-Streib (2010).",
        "Inferring the conservative causal core of gene regulatory networks.",
        "BMC Systems Biology.",
        "URL http://www.biomedcentral.com/1752-0509/4/132")
)
