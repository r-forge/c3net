citHeader("To cite c3net in publications use:")

citEntry(entry = "Article",
  title        = "C3NET: A superior open source R/Bioconductor Package for Mutual Information based Network Inference",
  author       = "Gokmen Altay and Frank emmert-Streib",
  journal      = "Bioinformatics",
  year         = "2010",
  volume       = "",
  url 		   = "http://bioinformatics.oxfordjournals.org/",
  textVersion  =
  paste("Gokmen Altay, Frank emmert-Streib (2010).",
        "C3NET: A superior open source R/Bioconductor Package for Mutual Information based Network Inference.",
        "Bioinformatics.",
        "URL http://bioinformatics.oxfordjournals.org/")
)
