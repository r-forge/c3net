citHeader("To cite c3net in publications use:")

citEntry(entry = "Article",
  title        = "C3NET: A superior open source R Package for Mutual Information based Network Inference",
  author       = "Gokmen Altay and Frank emmert-Streib",
  journal      = "Submitted",
  year         = "2010",
  volume       = "",
  url 		   = "",
  textVersion  =
  paste("Gokmen Altay, Frank emmert-Streib (2010).",
        "C3NET: A superior open source R Package for Mutual Information based Network Inference.",
        "Journal name.",
        "URL http://")
)
